{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    }
  }
}